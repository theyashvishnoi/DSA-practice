question link - https://leetcode.com/problems/concatenation-of-array/


class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        // create a vector ans
        vector<int> ans;
        //copy nums array to ans
        ans = nums;
        
        //push all the elements of nums array to ans array again
        for(int i=0; i<nums.size();i++){
            ans.push_back(nums[i]);
                
        }
        // return  ans array
        return ans;
    }
};
